{
  "builders": [
    {
      "type": "vagrant",
      "source_path": "./windows_2019_virtualbox.box",
      "output_directory": "./output-matlab/",
      "vm_name": "win2019-matlab",
      "communicator": "winrm",
      "winrm_password": "vagrant",
      "winrm_timeout": "6h",
      "winrm_username": "vagrant"
    }
  ],
  "provisioners": [
    {
      "type": "powershell",
      "inline": [
        "echo 'Do windows updates here'"
      ]
    },
    {
      "type": "file",
      "source": "/apps/MATLAB/R2021a",
      "destination": "C:\\tmp\\"
    }
  ]
}
